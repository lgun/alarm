<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.mybatis.admin">

    <select id="selectAiPlanMsg" parameterType="Map" resultType="Map">
        SELECT msg
        FROM aiplanmsg
        limit 1
    </select> 
    <update id="updateAiPlanMsg">
        update aiplanmsg
        set msg = #{msg}
    </update>
    <select id="selectAiPlanUser" parameterType="Map" resultType="Map">
        SELECT *
        FROM aiplanuserlist
        where userIdx=#{userIdx}
        limit 1
    </select> 
    <insert id="insertAiPlanUser">        
        INSERT INTO aiplanuserlist( 
            userIdx
            , adate
        ) VALUES (
            #{userIdx} 
            , now() 
        )
    </insert>
</mapper>